/* ハンバーガー */
@media screen and (max-width: 640px) {
  .menu-btn {
    position: fixed;
    top: 10px;
    right: 10px;
    display: flex;
    height: 60px;
    width: 60px;
    justify-content: center;
    align-items: center;
    z-index: 90;
    background-color: #3584bb;
  }
  .menu-btn span,
  .menu-btn span:before,
  .menu-btn span:after {
    content: '';
    display: block;
    height: 3px;
    width: 25px;
    border-radius: 3px;
    background-color: #ffffff;
    position: absolute;
  }
  .menu-btn span:before {
    bottom: 8px;
  }
  .menu-btn span:after {
    top: 8px;
  }
}

.c-btn-menu {
  display: none;
}
@media screen and (max-width: 640px) {
  .c-btn-menu {
    display: block;
    width: 68px;
    height: 68px;
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 17px;
    border: none;
    appearance: none;
    outline: none;
    position: fixed;
    top: 0;
    right: 0;
    z-index: 10000;
    background: #005B94;
  }
}
.c-btn-menu_line {
  width: 100%;
  height: 2px;
  border-radius: 4px;
  display: block;
  transition: 0.4s;
  background: #ffffff;
}
.c-btn-menu_line::before, .c-btn-menu_line::after {
  position: absolute;
  display: block;
  border-radius: 4px;
  width: 100%;
  height: 100%;
  content: "";
  transition: inherit;
  background: inherit;
}
.c-btn-menu_line::before {
  top: -10px;
}
.c-btn-menu_line::after {
  top: 10px;
}
.c-btn-menu.active {
  background: transparent;
}

.c-btn-menu.active .c-btn-menu_line {
  background-color: transparent;
}

.c-btn-menu.active .c-btn-menu_line::before, .c-btn-menu.active .c-btn-menu_line::after {
  top: 0;
  background-color: #fff;
}

.c-btn-menu.active .c-btn-menu_line::before {
  transform: rotate(-45deg);
}

.c-btn-menu.active .c-btn-menu_line::after {
  transform: rotate(45deg);
}

.p-hero_title {
  width: 42.3611111111%;
  position: absolute;
  text-align: center;
  top: calc(50% - 12px);
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 2;
}
@media screen and (max-width: 640px) {
  .p-hero_title {
    width: 272px;
  }
}
.p-hero_scroll {
  position: absolute;
  bottom: 28px;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 2;
  width: 50px;
  height: 50px;
  border: 1px solid #ffffff;
  border-radius: 50%;
}
@media screen and (max-width: 640px) {
  .p-hero_scroll {
    width: 44px;
    height: 44px;
  }
}
.p-hero_scroll::after {
  position: absolute;
  top: 50%;
  left: 50%;
  content: "";
  width: 16px;
  height: 16px;
  margin: -10px 0 0 -9px;
  border-left: 1px solid #ffffff;
  border-bottom: 1px solid #ffffff;
  transform: rotate(-45deg);
}
@media screen and (max-width: 640px) {
  .p-hero_scroll::after {
    width: 14px;
    height: 14px;
    margin: -10px 0 0 -8px;
  }
}
.p-hero_scroll::before {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  content: "";
  width: 48px;
  height: 48px;
  box-shadow: 0 0 0 0 rgba(255, 255, 255, 0.1);
  border-radius: 100%;
  opacity: 0;
  animation: sdb 2s infinite;
}
@media screen and (max-width: 640px) {
  .p-hero_scroll::before {
    width: 42px;
    height: 42px;
  }
}
.p-hero_scroll span {
  position: absolute;
  margin: 64px 0 0 6px;
  color: #ffffff;
  font-size: 12px;
}
@media screen and (max-width: 640px) {
  .p-hero_scroll span {
    margin: 56px 0 0 6px;
    font-size: 11px;
  }
}

