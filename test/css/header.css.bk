/* ヘッダー */
.header {
  display: flex;
  box-sizing: border-box;
  width: 100%;
  height: 8vw;
  position: fixed;
  top: 0;
  z-index: 1000;
  justify-content: space-between;
  align-items: center;
  /* padding: 0 3.8888888889%; */
  padding: 0 1vw;
  transition: 0.3s;
}
@media screen and (min-width: 960px) {
  .header {
    display: inline-block;
    align-items: center;
    height: 68px;
    margin-top: 0px;
    padding-top: 0px;
  }
  .header_contents {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.6s ease vasibility 0.6s ease;
  }
}

.header_contents .active {
  width: 100%;
  height: 100vh;
  z-index: 999;
  opacity: 1;
  visibility: visible;
  right: 0;
  transition: inherit;
  background: #110d0d;
}
.header_logo {
  z-index: 1000;
  width: 15vw;
}
