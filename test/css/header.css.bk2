/* ヘッダー */
.l-header {
  width: 100%;
  height: 84px;
  position: fixed;
  /* top: 0; */
  z-index: 1000;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 2vw 1vw 0;
  transition: 0.3s;
}
@media screen and (max-width: 960px) {
  .l-header {
    display: flex;
    height: 68px;
    margin-top: 0px;
    padding-top: 0px;
  }
  .l-header-nav {
    position: absolute;
    top: 0;
    left: 0;
    opacity: 0;
    visibility: hidden;
    transition: opacity 0.6s ease vasibility 0.6s ease;
  }
}
.l-header-nav.active {
  width: 100%;
  height: 100vh;
  z-index: 999;
  opacity: 1;
  visibility: visible;
  right: 0;
  transition: inherit;
  background: #110d0d;
}
.l-header-logo {
  display: inline-block;
  z-index: 1000;
  width: 20vw;
}
@media screen and (max-width: 960px) {
  .l-header-logo {
    display: inline-block;
    width: 200px;
    padding-top: 0px;
    margin-top: 0px;
  }
}
.l-header.scroll-nav {
  background: #ffffff;
  box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.1);
}

.l-subheader {
  background: #ffffff;
  box-shadow: 0 0.5rem 1rem rgba(0, 0, 0, 0.1);
}

.l-section {
  padding-top: 12.5%;
}

.l-subsection {
  padding: 180px 0;
}
@media screen and (max-width: 960px) {
  .p-global-nav {
    padding-top: 86px;
  }
}
.p-global-nav-list {
  display: flex;
  align-items: center;
  /* margin-left: 30px; */
}
@media screen and (max-width: 960px) {
  .p-global-nav-list {
    flex-direction: column;
    margin-left: 0;
  }
}
@media screen and (max-width: 960px) {
  .p-global-nav-item {
    width: 100%;
    text-align: center;
  }
}
/* @media screen and (max-width: 960px) {
  .p-global-nav-item:first-of-type {
    border-top: solid 1px #ffffff;
  }
}
*/
@media screen and (max-width: 960px) {
  .p-global-nav-item:last-of-type {
    margin-top: 40px;
  }
}

/* ヘッダーリンク */
.p-global-nav-item {
  font-size: 1.3vw;
}

.p-global-nav-item:not(:last-of-type) {
  margin-right: 3vw;
}
@media screen and (max-width: 960px) {
  .p-global-nav-item:not(:last-of-type) {
    margin-right: 0;
    display: block;
    border-bottom: solid 1px #ffffff;
  }
}
@media screen and (max-width: 960px) {
  .p-global-nav-item:last-of-type {
    margin-top: 40px;
  }
}
.p-global-nav-link {
  display: block;
  text-align: center;
}
@media screen and (max-width: 960px) {
  .p-global-nav-link {
    font-size: 14px;
  }
}
@media screen and (max-width: 960px) {
  .p-global-nav-link {
    padding: 24px 0;
    color: #ffffff;
  }
}

/* ホバーで色を変化 */
.p-global-nav-link:hover {
  color: #0084ad;
}

/* ヘッダーリンク見出し下の英語 */
.p-global-nav-sublink {
  display: none;
}
@media screen and (min-width: 960px) {
  .p-global-nav-sublink {
    display: block;
    margin-top: 6px;
    color: #686f75;
    transition: 0.3s;
    font-size: 10px;
  }
}
@media screen and (min-width: 1280px) {
  .p-global-nav-sublink {
    font-size: 14px;
  }
}

/* お問い合わせボタン */
.p-global-nav-contact-link {
  width: 192px;
  padding: 15px 0;
  display: inline-block;
  border-radius: 50px;
  background: #005b94;
  transition: 0.3s;
  text-align: center;
  font-size: 15px;
}
.p-global-nav-contact-link:hover {
  background: #0084ad;
}
/* 「お問い合わせ」の文字 */
.p-global-nav-contact-link span {
  top: -3px;
  left: 2px;
  color: #ffffff;
}

.p-global-nav-link:hover .p-global-nav-sublink {
  color: #0084ad;
}


