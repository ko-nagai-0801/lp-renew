.u-cts {
  position: relative;
}

.u-center {
  text-align: center;
}

/* ハンバーガーメニューボタン */
.c-btn-menu {
  display: block;
  width: 68px;
  height: 68px;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 17px;
  border: none;
  appearance: none;
  outline: none;
  position: fixed;
  top: 0;
  right: 0;
  z-index: 10000;
  background: #005b94;
}

/* 画面幅960px以上はハンバーガーボタン。メニューは非表示 */
@media screen and (min-width: 960px) {
  .c-btn-menu {
    display: none;
  }
}

.c-btn-menu-line {
  width: 100%;
  height: 2px;
  border-radius: 4px;
  display: block;
  transition: 0.4s;
  background: #ffffff;
}
.c-btn-menu-line::before,
.c-btn-menu-line::after {
  position: absolute;
  display: block;
  border-radius: 4px;
  width: 100%;
  height: 100%;
  content: "";
  transition: inherit;
  background: inherit;
}
.c-btn-menu-line::before {
  top: -10px;
}
.c-btn-menu-line::after {
  top: 10px;
}
.c-btn-menu.active {
  background: transparent;
}

.c-btn-menu.active .c-btn-menu-line {
  background-color: transparent;
}

.c-btn-menu.active .c-btn-menu-line::before,
.c-btn-menu.active .c-btn-menu-line::after {
  top: 0;
  background-color: #fff;
}

.c-btn-menu.active .c-btn-menu-line::before {
  transform: rotate(-45deg);
}

.c-btn-menu.active .c-btn-menu-line::after {
  transform: rotate(45deg);
}
