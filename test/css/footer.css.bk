  .l-footer-nav_list {
    display: flex;
    flex-wrap: wrap;
    z-index: 100;
    List-style: none;
    padding: 0;
  }
  .l-footer-nav_title {
    color: #ffffff;
    width:fit-content;
  }
  .l-footer-nav_title::after {
    position: absolute;
    left: 0;
    content: "";
    width: 100%;
    height: 1px;
    display: inline-block;
    background: #ffffff;
    bottom: -2px;
    transform: scale(0, 1);
    transform-origin: top left;
    transition: transform 0.4s;
  }
  .l-footer-nav_title:hover::after {
    transform: scale(1, 1);
  }
  .l-footer-nav_item {
    width: 200px;
    /* width: 170px; */
    margin-bottom: 7vw;
  }
  @media screen and (max-width: 640px) {
    .l-footer-nav_item {
      margin-bottom: 42px;
    }
  }
  .l-footer-nav_item:nth-of-type(2) {
    display: flex;
    flex-direction: column;
  }
  .l-footer-nav_link {
    width: fit-content;
    color: #ffffff;
    font-size: 14px;
    line-height: 30px;
    display: inline-block;
  }
  .l-footer-nav_link::after {
    position: absolute;
    left: 0;
    content: "";
    width: 100%;
    height: 1px;
    background: #ffffff;
    bottom: -2px;
    transform: scale(0, 1);
    transform-origin: top left;
    transition: transform 0.4s;
  }
  .l-footer-nav_link:hover::after {
    transform: scale(1, 1);
  }
  @media screen and (max-width: 640px) {
    .l-footer-nav_link {
      font-size: 11px;
    }
  }
  .l-footer-nav_link:nth-of-type(1) {
    margin-bottom: 16px;
  }
